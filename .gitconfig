[user]
	name = Kamil Dzieli≈Ñski
	email = brego.dk+d428c@gmail.com
[core]
	excludesfile   = ~/.gitignore
	attributesfile = ~/.gitattributes
[alias]
	a      = add
	ap     = add -p
	b      = branch
	c      = commit
	cm     = commit -m
	co     = checkout
	cb     = checkout -b
	d      = diff
	ds     = diff --staged
	f      = fetch
	h      = log --pretty=format:'%C(yellow)%h%Creset %cd | %s%C(yellow)%d%Creset %C(red)(%an, %cr)%Creset' --graph --date=short
	l      = log --name-status
	log    = log --topo-order
	m      = merge
	r      = rebase
	s      = status -s
	up     = pull
	fuckit = reset --hard HEAD
[pull]
	ff     = only
	rebase = true
[push]
	default = simple
[rerere]
	enabled = true
[color]
	ui = always
[color "branch"]
	current = yellow reverse
	local   = yellow
	remote  = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old  = red bold
	new  = green bold
[color "status"]
	added     = yellow
	changed   = green
	untracked = cyan
[merge]
	log = true
[url "git@github.com:"]
	insteadOf     = "gh:"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
	insteadOf = "github:"
[url "git@gist.github.com:"]
	insteadOf     = "gst:"
	pushInsteadOf = "gist:"
	pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
	insteadOf = "gist:"
